{
  "watch": ["server", "src/lib"],
  "ext": "ts,js,json",
  "ignore": ["node_modules", "dist"],
  "exec": "node --loader ts-node/esm server/server.ts",
  "env": {
    "NODE_ENV": "development",
    "TS_NODE_PROJECT": "tsconfig.server.json"
  },
  "nodeArgs": ["--experimental-specifier-resolution=node"]
}
